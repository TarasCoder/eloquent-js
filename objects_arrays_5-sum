const range = (start, end, step = 1) => {
    let arr = [];
    for (let i = start; i < end + 1 ; i += step) {
        arr.push(i);
    }
    return arr;
}
const sum = (arr) => {
    let sum = 0;
    for (number of arr){
        sum += number;
    }
    return sum;
}
console.log(sum(range(1,10,2)));
